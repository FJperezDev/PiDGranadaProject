{
  "expo": {
    "name": "frontendStudent",
    "slug": "frontendStudent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/studentIcon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/studentIcon.png",
        "backgroundColor": "#003594"
      },
      "package": "com.fran1763.frontendStudent",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "./withManifestFix.js",
      "./withDependencyFix.js",
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "Permite el acceso al micr√≥fono...",
          "speechRecognitionPermission": "Permite el reconocimiento de voz..."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "useAndroidX": true,
            "enableJetifier": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "6a01833f-8e6a-4c56-b268-92f9447461e9"
      }
    }
  }
}
